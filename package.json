{
  "name": "mask-optimizer",
  "version": "0.1.6",
  "description": "MaskJS Minifier",
  "author": "Alexander Kit (alex.kit@atmajs.com)",
  "main": "module.js",
  "license": "MIT",
  "scripts": {
    "test": "atma test test/**.test"
  },
  "devDependencies": {
    "atma": ">0.9.30",
    "atma-loader-babel": "^1.0.19"
  },
  "dependencies": {
    "atma-io": "^0.2.6",
    "maskjs": "^0.12.17"
  },
  "atma": {
    "plugins": [
      "atma-loader-babel"
    ],
    "settings": {
      "atma-loader-babel": {
        "extensions": [
          "es6",
          "test"
        ]
      }
    }
  }
}
